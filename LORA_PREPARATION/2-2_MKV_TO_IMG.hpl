        <jsScript_script>
function removeExtension(filename) {
    var lastDotPosition = filename.lastIndexOf(".");
    if (lastDotPosition === -1) {
        return filename; // Pas d'extension trouv√©e
    } else {
        return filename.substr(0, lastDotPosition);
    }
}

var cmd_exec           = "cmd";
var cmd_option         = "/c";
var cmd_ffmpeg         = "ffmpeg -loglevel error -y";
var cmd_ffmpeg_option1 = '-i "' + filename + '"';
var cmd_ffmpeg_option2 = "-vf";
var cmd_ffmpeg_option3 = '"fps=18,select=not(mod(n\\,5))"';  
var cmd_ffmpeg_option4 = "-q:v";
var cmd_ffmpeg_option5 = "1";
var cmd_ffmpeg_option6 = "-fps_mode";
var cmd_ffmpeg_option7 = "vfr";
var cmd_ffmpeg_option8 = '"'+removeExtension(filename)+ '\\frame_%08d.jpg"';


var cmd_ffmpeg_complete = cmd_ffmpeg + " " + cmd_ffmpeg_option1 + " " + cmd_ffmpeg_option2 + " " + cmd_ffmpeg_option3 + " " + cmd_ffmpeg_option4 + " " + cmd_ffmpeg_option5 + " " + cmd_ffmpeg_option6 + " " + cmd_ffmpeg_option7 + " " + cmd_ffmpeg_option8;</jsScript_script>
      </jsScript>
    </jsScripts>
